<body background="assets/BACKGROUND.jpg">

<div class="wrapper">
	<div id="username"><input type="text" name="username" size="30"></div>
</div>

<button type="image" name="run_button" onClick="Launch()" value="Go" id="submit" class="btn btn-primary btn-success"> </button>

<input type="password" name="password" id="password" size="30">
</body>
<script Language="VBScript">
	Sub Window_Onload
	  window.resizeTo 1220,637
	End Sub
</script>
<script Language="VBScript">
    Sub Launcher
        Dim objShell
        set objShell = CreateObject ( "WScript.Shell" )
        set sysenv = objShell.Environment ( "PROCESS" )
	    sysenv("TTI_PLAYCOOKIE") = username.Value&":"&password.Value
        sysenv("TTI_GAMESERVER") = "108.67.132.207 "
        objShell.currentDirectory = objShell.currentDirectory&"\.."
        objShell.Run("C:/Panda3D-1.9.0/python/ppython.exe -m toontown.toonbase.ToontownPlanetStart")
		Set objFileToWrite = CreateObject("Scripting.FileSystemObject").OpenTextFile("test.txt",2,true)
        objFileToWrite.WriteLine("password.Value")
		objFileToWrite.Close
		Set objFileToWrite = Nothing
        set objShell = Nothing
        Me.Close
    End Sub
</script>
